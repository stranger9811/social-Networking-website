







<div class="container">
  <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <div class="well well-sm">
        <%= form_for(@user) do |form| %>
          <fieldset>
          
          
          
          <div class="form-group">
                <label class="col-md-3 control-label" for="message">Timeline Pic</label>
                 <div class="col-md-9">
                    <%= form.file_field :timeline_pic %>
                </div>
            </div>
            <div class="form-group">
                <label class="col-md-3 control-label" for="message">Profile Pic</label>
                 <div class="col-md-9">
                    <%= form.file_field :profile_pic %>
                </div>
            </div>

            <div class="form-group">
              <div class="col-md-12 text-right">
           <%= form.submit 'Create',:class=>"btn btn-default btn-info btn-lg btn-block" %>
           </div>
            </div>

          </fieldset>
        <% end %>
    </div>
  </div>
  </div>
</div>


