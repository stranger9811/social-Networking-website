<!DOCTYPE html>
<html>
<head>
  <title>DatabaseProject</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <link type="text/css" rel="stylesheet" href="/css/stylish_div.css" />
  <%= csrf_meta_tags %>
</head>
<body style="background-color: 233,234,237;">
<form id="search_form"> <input id="search_box" type="text" name="search_box"/></form>
<%= yield %>
<script type = "/text/javascript">
$(document).ready(function() {
    $(function() {
        $("#search_box").autocomplete({
            source: ['Google', 'Yahoo', 'StackOverflow'],
            select: function(event, ui) {
                $(event.target).val(ui.item.value);
                $('#search_form').submit();
                return false;
            },
            minLength: 1
        });
    });
});
</script>
</body>
</html>
